/**
 * Not gate:
 * if (in) out = 0, else out = 1
 */
 //in hdl there is not order required you can put the chips into any order you wanted 
CHIP Not {
    IN in;
    OUT out;

    PARTS:
    Nand(a=in, b=in, out=out);
    
}